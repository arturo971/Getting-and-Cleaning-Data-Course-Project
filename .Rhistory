getr<-function(price){
R<-NULL
if (is.data.frame(price)){
for (i in 2:nrow(price)){
f<-(price[i,1]/price[i-1,1])-1
R<-rbind(R,f)
}
r<-sapply(R,function(x) log(x+1))
r
}
else if (is.vector(price)){
for (i in 2:length(price)){
f<-(price[i]/price[i-1])-1
R<-rbind(R,f)
}
r<-sapply(R,function(x) log(x+1))
}
r<-c(NA,r)
names(r)<-paste("r_",price,sep="")
}
